peline {

    //agent any
    agent { dokcer {image 'maven:3.6.3'} }
    stages{
        stage('Build') {
            steps {
                sh 'mvn --version'
                echo "Build"
            }
        }
        stage('Test') {
            steps {
                echo "Test"
            }
        }
        stage('IntegrationTest') {
            steps {
                echo "IntegrationTest"
            }
        }
    }
}

